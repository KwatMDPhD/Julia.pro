    "# `isequal` checks hashs.\n",
    "\n",
    "for (an1, an2) in ((missing, missing), (NaN, NaN), (Inf, Inf), (0, 0.0), (-0.0, 0.0))\n",
    "\n",
    "    println(\"$an1 ($(hash(an1))) and $an2 ($(hash(an2)))\")\n",
    "\n",
    "    println(an1 == an2)\n",
    "\n",
    "    println(isequal(an1, an2))\n",
    "\n",
    "end"
